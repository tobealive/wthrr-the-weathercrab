{
  assignees: [
    "kevinmatthes",
  ],
  automerge: false,
  commitMessageAction: "update",
  commitMessageLowerCase: "never",
  commitMessagePrefix: "⬆️",
  dependencyDashboard: true,
  dependencyDashboardAutoclose: true,
  dependencyDashboardLabels: [
    "dependencies",
  ],
  labels: [
    "dependencies",
  ],
  lockFileMaintenance: {
    commitMessageAction: "maintain Cargo.lock",
    commitMessagePrefix: "🤖",
    enabled: true,
  },
  packageRules: [
    {
      addLabels: [
        "rust",
      ],
      matchManagers: [
        "cargo",
      ],
    },
    {
      addLabels: [
        "github_actions",
      ],
      matchManagers: [
        "github-actions",
      ],
    },
  ],
  prHourlyLimit: null,
  schedule: "before 4am on monday",
}
